{
  "name": "chrome-extension2",
  "displayName": "Google Photos Deduper",
  "version": "1.0.0",
  "description": "Chrome extension to remove duplicates from Google Photos",
  "author": "Mack Talcott",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.6.2",
    "plasmo": "0.89.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "activeTab",
      "windows",
      "tabs",
      "scripting"
    ],
    "host_permissions": [
      "https://*/*",
      "http://*/*",
      "https://photos.google.com/*"
    ]
  }
}
